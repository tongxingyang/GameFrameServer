<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Common"
                   namespace="Common.Model">

  <class name="PlayerInfoModel" table="playerinfoserver">
    <id name="ID" column="id" type="Int32">
      <generator class="native"></generator>
    </id>
    <property name="AccountID" column="userid" type="Int32"></property>
    <property name="RoleName" column="rolename" type="String"></property>
    <property name="LV" column="lv" type="Int32"></property>
    <property name="EXP" column="exp" type="Int32"></property>
    <property name="Power" column="power" type="Int32"></property>
    <property name="HeroID" column="hero" type="String"></property>
    <property name="FriendID" column="firend" type="String"></property>
    <property name="CoinNum" column="coinnum" type="Int32"></property>
    <property name="CouponNum" column="couponnum" type="Int32"></property>
    <property name="WinNum" column="winnum" type="Int32"></property>
    <property name="LostNum" column="lostnum" type="Int32"></property>
    <property name="RunNum" column="runnum" type="Int32"></property>
    <property name="HeadImg" column="headimg" type="String"></property>
  </class>

</hibernate-mapping>